rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /feedback/{feedbackId} {
      allow read: if false;
      allow write: if false;
    }

    match /canteens/{canteenId} {
      allow read: if true;
      allow write: if false;

      match /reviews/{reviewId} {
        allow read: if true;
        allow write: if false;
      }
    }
  }
}
